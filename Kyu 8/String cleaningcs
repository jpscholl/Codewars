using System.Text.RegularExpressions;

Console.WriteLine("h3ll0 w0rld!");
Console.WriteLine(StringClean("h3ll0 w0rld!"));

static string StringClean(string s)
{
    return Regex.Replace(s, @"[\d-]", string.Empty);
}
